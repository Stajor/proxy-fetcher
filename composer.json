{
    "name": "stajor/proxy-fetcher",
    "description": "PHP package for dealing with proxy lists from various sources",
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^7.0",
        "fzaninotto/faker": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "ProxyFetcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProxyFetcher\\Test\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
