{
    "name": "stajor/proxy-fetcher",
    "description": "PHP package for dealing with proxy lists from various sources",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "fakerphp/faker": "^1.11",
        "symfony/css-selector": "^5.2",
        "symfony/dom-crawler": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "ProxyFetcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProxyFetcher\\Test\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit tests --filter=SslproxiesOrgTest"
    }
}
